{
  "name": "polarize/bladevue",
  "description": "Trailblazer method utilized for Laravel.",
  "license": "MIT",
  "authors": [
      {
          "name": "Joshua Anderton",
          "email": "hey@joshuaanderton.ca"
      }
  ],
  "require": {
      "php": "^8.0",
      "illuminate/support": "^8.67|^9.0"
  },
  "require-dev": {
      "phpunit/phpunit": "^9.3"
  },
  "autoload": {
      "psr-4": {
          "Polarize\\Bladevue\\": "src"
      }
  },
  "autoload-dev": {
      "psr-4": {
          "Polarize\\Bladevue\\Tests\\": "tests"
      }
  },
  "config": {
      "sort-packages": true
  },
  "extra": {
      "laravel": {
          "providers": [
              "Polarize\\Bladevue\\Providers\\BladevueServiceProvider"
          ]
      }
  }
}